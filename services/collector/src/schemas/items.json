 {
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "https://raw.githubusercontent.com/cortexteam/code-test/master/resources/input.json",
   "type": "array",
   "title": "Input Schema",
   "items": {
     "$id": "#/items",
     "type": "object",
     "title": "The Items Schema",
     "required": [
       "responseNumber",
       "originalInvoiceNumber",
       "status",
       "date",
       "amount",
       "currency"
     ],
     "properties": {
       "responseNumber": {
         "$id": "#/items/properties/responseNumber",
         "type": "string",
         "title": "The Responsenumber Schema",
         "default": "",
         "examples": [
           "1264"
         ],
         "pattern": "^(.*)$"
       },
       "originalInvoiceNumber": {
         "$id": "#/items/properties/originalInvoiceNumber",
         "type": "string",
         "title": "The Originalinvoicenumber Schema",
         "default": "",
         "examples": [
           "879755"
         ],
         "pattern": "^(.*)$"
       },
       "status": {
         "$id": "#/items/properties/status",
         "type": "string",
         "title": "The Status Schema",
         "default": "",
         "examples": [
           "Rejected"
         ],
         "pattern": "^(.*)$"
       },
       "date": {
         "$id": "#/items/properties/date",
         "type": "string",
         "title": "The Date Schema",
         "default": "",
         "examples": [
           "2016-10-14"
         ],
         "pattern": "^(.*)$"
       },
       "amount": {
         "$id": "#/items/properties/amount",
         "type": "string",
         "title": "The Amount Schema",
         "default": "",
         "examples": [
           "2481.86"
         ],
         "pattern": "^(.*)$"
       },
       "currency": {
         "$id": "#/items/properties/currency",
         "type": "string",
         "title": "The Currency Schema",
         "default": "",
         "examples": [
           "USD"
         ],
         "pattern": "^(.*)$"
       }
     }
   }
 }
